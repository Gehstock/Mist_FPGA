        <dip name="4 Player Cabinet" bits="3" ids="Table,4P"/>
        <dip name="2 Player Cabinet" bits="7" ids="Table,2P"/>
        <!-- IN4 -->
        <dip name="Difficulty" bits="8,9" ids="Very Hard,Hard,Normal,Easy"/>
